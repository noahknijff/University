library(ggplot)
install.packages("ggplot2")
install.packages("reshape2")
cholesterol = read.table("cholesterol.txt", header=TRUE)
# Convert data to long format for ggplot
cholesterol_long <- reshape2::melt(cholesterol, variable.name = "Time", value.name = "Value")
# Create histogram
ggplot(cholesterol_long, aes(x = Value, fill = Time)) +
geom_histogram(alpha = 0.5, position = "identity", bins = 10) +
labs(title = "Cholesterol Levels Before and After 8 Weeks",
x = "Cholesterol Level",
y = "Count") +
theme_minimal() +
scale_fill_manual(values = c("blue", "red"))
find.package("ggplot2")
